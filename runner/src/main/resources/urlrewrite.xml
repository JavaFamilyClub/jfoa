<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
   PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
   "http://www.tuckey.org/res/dtds/urlrewrite3.0.dtd">

<urlrewrite>

   <rule>
      <from>^/app/(?!.*\.+.+).*$</from>
      <to type="redirect">/app/index.html</to>
   </rule>

</urlrewrite>
