{
  "name": "jfoa-web",
  "version": "0.0.1",
  "scripts": {
    "ng": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng",
    "start": "npm run ng -- serve",
    "build": "npm run build:ng && npm run build:resources",
    "build:prod": "npm run build:ng:prod && npm run build:resources",
    "build:ng": "npm run build:ng:app",
    "build:ng:app": "npm run ng -- build",
    "build:ng:prod": "npm run build:ng:app -- --prod --aot",
    "build:ng:installer": "npm run build:ng:app -- --aot --configuration=installer",
    "build:watch": "npm run build:ng:app -- --watch",
    "build:resources": "gulp",
    "i18n": "gulp i18n",
    "lint": "tsc -p rules/tsconfig.json && npm run ng -- lint",
    "generate-index-files": "node ./generate-index-files.js",
    "test": "npm run ng -- test jfoa-web --ci --runInBand --reporters=default --reporters=jest-junit",
    "test:watch": "npm run test -- --watch",
    "report-coverage": "codecov --token=1eb18219-afec-47c9-8f90-1210b4ec4d07",
    "installer:compile:app": "npm run build:ng:installer -- --outputPath=./build/client --crossOrigin=anonymous --baseHref=./",
    "installer:compile:app:watch": "npm run installer:compile:app -- --watch",
    "installer:compile": "concurrently -r \"npm run build:resources\" \"npm run installer:compile:app\"",
    "installer:packagingOsx": "electron-packager ./build/client jfoa-client --platform=darwin --arch=x64 --icon=build/client/favicon.icns --app-version=0.0.1 --asar --overwrite --out=./build/installer",
    "installer:packagingLinux": "electron-packager ./build/client jfoa-client --platform=linux --arch=x64 --app-version=0.0.1 --asar --overwrite --out=./build/installer",
    "installer:packagingLikeLinux": "npm run installer:packagingLinux",
    "installer:packagingWin64": "electron-packager ./build/client jfoa-client --platform=win32 --arch=x64 --app-version=0.0.1 --asar --overwrite --out=./build/installer",
    "installer:packaging": "electron-packager ./build/client jfoa-client --all --app-version=0.0.1 --asar --overwrite --out=./build/installer",
    "build:installer": "npm run installer:compile && npm run installer:packagingOsx",
    "build:installer:likeLinux": "npm run installer:compile && npm run installer:packagingLikeLinux",
    "build:installer:windows": "npm run installer:compile && npm run installer:packagingWin64"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.5",
    "@angular/cdk": "^10.1.0",
    "@angular/common": "^10.0.5",
    "@angular/compiler": "^10.0.5",
    "@angular/core": "^10.0.5",
    "@angular/forms": "^10.0.5",
    "@angular/localize": "^10.0.5",
    "@angular/material": "^10.1.0",
    "@angular/material-moment-adapter": "^10.1.3",
    "@angular/platform-browser": "^10.0.5",
    "@angular/platform-browser-dynamic": "^10.0.5",
    "@angular/router": "^10.0.5",
    "@mdi/font": "^4.7.95",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "angular-froala-wysiwyg": "^3.2.1",
    "bootstrap": "^4.5.0",
    "bootstrap-float-label": "4.0.2",
    "browserslist": "^4.8.2",
    "core-js": "^3.6.5",
    "echarts": "^4.6.0",
    "jest-codemods": "^0.22.1",
    "jquery": "^3.5.1",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "net": "^1.0.2",
    "roboto-fontface": "0.10.0",
    "rxjs": "^6.5.5",
    "sockjs-client": "^1.5.0",
    "split.js": "^1.5.11",
    "stompjs": "^2.3.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^8.3.2",
    "@angular-devkit/build-angular": "~0.1000.4",
    "@angular/cli": "~10.0.4",
    "@angular/compiler-cli": "^10.0.5",
    "@angular/language-service": "^10.0.5",
    "@types/geojson": "^1.0.0",
    "@types/jest": "^24.9.1",
    "@types/jquery": "^3.5.0",
    "@types/node": "^12.11.1",
    "@types/sockjs": "^0.3.32",
    "clean-webpack-plugin": "1.0.1",
    "codecov": "^3.5.0",
    "codelyzer": "^6.0.0",
    "concurrently": "^3.5.1",
    "electron": "^10.1.1",
    "electron-packager": "^15.1.0",
    "gulp": "^4.0.2",
    "gulp-filter": "^6.0.0",
    "gulp-hub": "^4.2.0",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.2.0",
    "jest-junit": "^6.3.0",
    "js-yaml": "^3.13.1",
    "rxjs-tslint": "^0.1.8",
    "through2": "^2.0.3",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.0",
    "typescript": "3.9.7",
    "vinyl": "^2.2.0",
    "vinyl-fs": "^2.4.4"
  }
}
